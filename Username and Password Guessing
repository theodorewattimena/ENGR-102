#Imputing Passwords and Usernames
list = []
list2 = []
print('Insert number of usernames and passwords:')
number = int(input('Number of Combinations:'))
for i in range (number):
    listname = str(input('Insert Username:'))
    listpassword = str(input('Insert Password:'))
    list.append(listname)
    list2.append(listpassword)
    combination = ({listname:listpassword})

#User Inserting Combo
print('Insert Username and Password Combination')
guessname = str(input('Insert Username:'))
guesspassword = str(input('Insert Password:'))
while True:
    if guessname in list and guesspassword in list2:#If Combination is Correct
        print('Access Granted')
        break
    else:
        print('Combination Wrong')
        print('Insert Username and Password Combination')
        guessname = str(input('Insert Username:'))
        guesspassword = str(input('Insert Password:'))
        
